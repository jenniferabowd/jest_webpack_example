{
  "name": "Example",
  "version": "1.0.0",
  "description": "This is an example of a complex app using Jest",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js --mode production --open --hot",
    "build": "webpack --progress --mode production",
    "watch": "webpack --watch --progress --mode production",
    "test": "jest"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/reactTests/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/client/src/stylesheets/"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/client/src/stylesheets/"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.19.3",
    "@babel/runtime": "^7.19.0",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.5",
    "body-parser": "^1.18.3",
    "file-loader": "^0.11.2",
    "nodemon": "^1.18.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "webpack": "^5.74.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.8",
    "jest": "^29.2.0",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^18.2.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  }
}
